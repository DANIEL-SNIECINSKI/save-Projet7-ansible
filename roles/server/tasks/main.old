---
# tasks file for server
- name: General | Installation des paquets requis
  apt:
    name: "{{ packages }}"
    state: installed
  vars:
    packages:
       - unzip
       - apache2
       - mysql-server
       - php7.2-mysql
       - php7.2
       - libapache2-mod-php7.2
       - python-mysqldb



